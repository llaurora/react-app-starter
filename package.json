  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.config.js",
    "mock:server": "nodemon ./mock/server.js",
    "dev:mock": "concurrently \"yarn run mock:server\" \"cross-env NODE_STAGE=mock yarn run dev\"",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.build.config.js --progress --hide-modules --display-optimization-bailout",
    "build:test0": "cross-env NODE_STAGE=test0 yarn run build",
    "eslint": "eslint 'src/**/*.{js,jsx}'",
    "stylelint": "stylelint 'src/**/*.s?(a|c)ss'",
    "lint": "yarn run eslint && yarn run stylelint",
    "eslint:fix": "yarn run eslint --fix",
    "stylelint:fix": "yarn run stylelint --fix",
    "lint:fix": "yarn run eslint:fix && yarn run stylelint:fix"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "express": "4.17.1",
    "memoize-one": "^5.1.1",
    "mockjs": "^1.1.0",
    "moment": "^2.24.0",
    "nodemon": "^1.19.4",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
