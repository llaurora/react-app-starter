  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn run eslint:fix",
      "git add"
    ],
    "src/**/*.s?(a|c)ss": [
      "yarn run stylelint:fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=9.10.0"
  }
